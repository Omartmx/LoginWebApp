<?xml version="1.0" encoding="UTF-8"?>
<Context path="/LoginWebApp">
    <Resource auth="Container" driverClassName="com.mysql.cj.jdbc.Driver" maxIdle="30" maxTotal="100" maxWaitMillis="10000" name="jdbc/mysql" password="${MYSQLPASSWORD}" type="javax.sql.DataSource" url="jdbc:mysql://${MYSQLHOST}:${MYSQLPORT}/${MYSQLDATABASE}?useSSL=false&amp;serverTimezone=UTC" username="${MYSQLUSER}"/>
</Context>
